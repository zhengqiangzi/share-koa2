<!doctype html>
<html>
<head>
	<meta charset="utf-8"/>
	<title>koa</title>
	
	<link type="text/css" rel="styleSheet" href="bootstrap.css"/>
	<style type="text/css">
		body{
				margin:0;
				padding: 0;
		}
		ul{
				margin: 0;
				padding: 0;
				list-style-type: none;
		}

	</style>
</head>
<body>




	<table class="table table-striped table-bordered table-hover">

		<tr>
			
			<th>ID</th>
			<th>Name</th>
			<th>Age</th>
			<th>sex</th>
			<th>address</th>
		</tr>

		{{#each data}}
			<tr>
				<td>{{this.id}}</td>
				<td>{{this.name}}</td>
				<td>{{this.age}}</td>
				<td>{{this.sex}}</td>
				<td>{{this.address}}</td>
			</tr>
		{{/each}}


  </table>
</body>
</html>